import java.util.Scanner;
public class reverse
{
    public static void main(String[] args)
    {
        Scanner in=new Scanner(System.in);
        System.out.print("Enter the String: ");
        char[] str=in.nextLine().toCharArray();
        int l=str.length;
        String rev="";
        for(int i=l-1;i>=0;i--)
        {
            rev=rev+str[i];
        }
        System.out.println(rev);
    }
}
